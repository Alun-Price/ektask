                <div class="flex items-center">
                    {#  Button - Hero #}
                    <button type="button" class="text-white bg-primaryorange-400 hover:bg-primaryorange-600 font-bold text-center text-[17px] px-5 mr-3 rounded-[35px] w-[152px] h-[52px]">What We Do</button>

                    <a href="#" class="flex items-center py-1 group">   
                        <svg class=" group-hover:text-primaryorange-600" width="28" height="29" viewBox="0 0 28 29" fill="none" 
                            xmlns="http://www.w3.org/2000/svg">
                            <circle cx="14" cy="14.4719" r="13" stroke="#F26839" stroke-width="2" fill="#F26839"/>
                            <path d="M12 18.8252L18.0674 14.9719L12 11.1186V18.8252Z" fill="#FFF" stroke="#FFF"/>
                            </svg>
                        <span class="px-2 text-[17px] font-bold text-primaryorange-400 group-hover:text-primaryorange-600">Watch Video</span>
                    </a>
                </div>
            </div>
             {# Image Section - Hero #}
            <div class="justify-center h-full align-center lg:flex lg:items-center md:flex ">
                <img class="md:hidden lg:hidden xl:hidden" <img src="{{ siteUrl }}static/images/smHero.svg">
                <img class="hidden md:block lg:hidden xl:hidden" <img src="{{ siteUrl }}static/images/mdHero.svg">
                <img class="md:hidden sm:hidden lg:block xl:hidden" <img src="{{ siteUrl }}static/images/lgHero.svg">
                <img class="absolute xl:top-[20px] xl:right-[50px] md:hidden sm:hidden lg:hidden xl:block" <img src="{{ siteUrl }}static/images/xlHero.svg">
            </div>
        </div>
    </section>
    
    {# ICON GRID SECTION #}
    <section class="w-full h-52">
        <div class="icon-block">
            <div class="container mx-auto mt-10 text-sm font-bold leading-6 text-center text-primaryblack-950">Trusted by organisations globally</div>
        </div>
        <div class="grid gap-1 md:gap-3 grid-row-1 auto-rows-fr">
            {% set smicons = entry.smlogos.limit(10).asArray().all() %}
            
            <div class="grid grid-cols-3 overflow-hidden md:grid-cols-7 lg:grid-cols-8 xl:grid-cols-9 justify-items-center">
                {% for smicon in smicons %}
                   
                   {{ smicon.title }}
                {% endfor %}
            </div>    
        </div>
        <div class="grid gap-1 md:gap-3 grid-row-1 auto-rows-fr">
            {% set mdicons = entry.mdlogos.limit(3).all() %}
            
            <div class="grid grid-cols-3 overflow-hidden md:grid-cols-7 lg:grid-cols-8 xl:grid-cols-9 justify-items-center">
                {% for mdicon in mdicons %}
                   <div class="hidden md:block">
                    {{ mdicon.title }}
                   </div>
                {% endfor %}
            </div>    
        </div>
    </section>
        
        
